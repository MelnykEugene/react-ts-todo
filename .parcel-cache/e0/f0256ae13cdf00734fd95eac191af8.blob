#list-container {
  margin: 0 auto;
  width: 50%;
  display: flex;
  flex-flow: column nowrap;
}
.item-content {
  display: inline;
}

.item-button {
  float: right;
  height: 10px;
  width: 10px;
  border: none;
  background-color: transparent;
}
.item-button:before {
  content: "x";
  color: gray;
}
.item-button:before:hover {
  color: rgba(51, 51, 51);
}

